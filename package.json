{
	"name": "svelte-stack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev-ws": "bunx nodemon --watch dev.ts --watch src/hooks.server.ts --exec 'bun --bun dev.ts'",
		"dev-db": "docker compose -f docker-compose.dev.yml up",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/bun": "^1.1.6",
		"@types/dom-view-transitions": "^1.0.4",
		"@types/nodemailer": "^6.4.15",
		"@types/pg": "^8.11.6",
		"@types/react": "^18.3.3",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.22.8",
		"nodemon": "^3.1.4",
		"postcss": "^8.4.39",
		"postcss-load-config": "^6.0.1",
		"postgres": "^3.4.4",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0-next.179",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite-plugin-inspect": "^0.8.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@react-email/components": "^0.0.19",
		"@react-email/tailwind": "^0.0.18",
		"@types/react-dom": "^18.3.0",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-de": "^3.0.2",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"@sveltejs/enhanced-img": "^0.3.3",
		"arctic": "^1.9.1",
		"argon2": "^0.40.3",
		"bits-ui": "^0.21.12",
		"clsx": "^2.1.1",
		"cookie": "^0.6.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.31.4",
		"drizzle-zod": "^0.5.1",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.407.0",
		"mode-watcher": "^0.3.1",
		"nodemailer": "^6.9.14",
		"oslo": "^1.2.1",
		"pg": "^8.12.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"server": "react-dom/server",
		"shadcn-svelte": "^0.10.7",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-sonner": "^0.3.25",
		"sveltekit-flash-message": "^2.4.4",
